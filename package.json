{
  "name": "hardhat-project",
  "devDependencies": {
    "@defi-wonderland/smock": "^2.0.7",
    "node-ts": "^5.1.2"
  },
  "dependencies": {
    "@0xsequence/erc-1155": "^3.0.4",
    "@chainlink/contracts": "^0.2.2",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.4.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@openzeppelin/hardhat-upgrades": "^1.16.1",
    "@typechain/ethers-v5": "^7.1.0",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4.3.4",
    "chainlink": "^0.8.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.6.2",
    "hardhat": "^2.9.6",
    "hardhat-deploy": "^0.8.11",
    "hardhat-gas-reporter": "^1.0.4",
    "path": "^0.12.7",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "starknet": "3.10.2",
    "truffle-assertions": "^0.9.2",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typechain": "^5.1.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "typechain": "node ./.setup.js && hardhat typechain",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile",
    "void:deploy": "hardhat deploy",
    "deploy-arb": "hardhat deploy --network arbitrumRinkeby",
    "deploy-rink": "hardhat deploy --network rinkeby",
    "test": "hardhat test",
    "gas": "cross-env REPORT_GAS=true hardhat test",
    "coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "dev": "hardhat node --watch --export contractsInfo.json",
    "local:dev": "hardhat --network localhost deploy --watch",
    "disburse": "hardhat disburse --network localhost"
  }
}
